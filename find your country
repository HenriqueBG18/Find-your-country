import plotly.express as px

country = input('Coloque o nome do pa√≠s(in english BITCH): ')
data = {
    'country': [country],
    'values': [100]
}
fig = px.choropleth(
    data,
    locations='country',
    locationmode='country names',
    color='values',
    color_continuous_scale='blues',
    title=f'Mapa pintado de: {country}')
fig.show()
